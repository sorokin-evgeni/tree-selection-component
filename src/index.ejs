<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-type" content="text/html; charset=utf-8"/>
    <title><%= htmlWebpackPlugin.options.title %></title>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
</head>
<body>
    <div id="content"></div>
    <div class="modal-wrapper" style="display: none">
        <div class="modal-wrapper__overlay"></div>
        <div class="modal">
            <header class="modal__header">
                Configure visible projects
                <a class="material-icons modal__close">close</a>
            </header>
            <section class="modal__section projects-form">
                <nav>
                    <a class="material-icons">undo</a>
                    <a class="material-icons">redo</a>
                </nav>
                <table>
                    <tr>
                        <td class="projects-cell">
                            <div class="projects">
                                <div class="projects__title">Visible projects:</div>
                                <ul>
                                    <li>
                                        <div class="projects__project-name">
                                            &lt;Root project&gt;
                                        </div>
                                        <ul>
                                            <li>
                                                <div class="projects__project-name">
                                                    assessment
                                                </div>
                                                <ul>
                                                    <li>
                                                        <div class="projects__project-name">
                                                            api
                                                        </div>
                                                    </li>
                                                    <li>
                                                        <div class="projects__project-name">
                                                            front
                                                        </div>
                                                    </li>
                                                    <li>
                                                        <div class="projects__project-name">
                                                            server
                                                        </div>
                                                    </li>
                                                </ul>
                                            </li>
                                        </ul>
                                    </li>
                                </ul>
                            </div>
                        </td>
                        <td class="projects-cell">
                            <div class="projects">
                                <div class="projects__title">Hidden projects:</div>
                                <div class="projects__search">
                                    <input class="input"/>
                                </div>
                                <ul>
                                    <li>
                                        <div class="projects__project-name">
                                            Audit
                                        </div>
                                        <ul>
                                            <li>
                                                <div class="projects__project-name">
                                                    security
                                                </div>
                                                <ul>
                                                    <li>
                                                        <div class="projects__project-name">
                                                            api
                                                        </div>
                                                    </li>
                                                    <li>
                                                        <div class="projects__project-name">
                                                            front
                                                        </div>
                                                    </li>
                                                    <li>
                                                        <div class="projects__project-name">
                                                            server
                                                        </div>
                                                    </li>
                                                </ul>
                                            </li>
                                            <li>
                                                <div class="projects__project-name">
                                                    base
                                                </div>
                                            </li>
                                            <li>
                                                <div class="projects__project-name">
                                                    common
                                                </div>
                                            </li>
                                        </ul>
                                    </li>
                                    <li>
                                        <div class="projects__project-name">
                                            Development
                                        </div>
                                        <ul>
                                            <li>
                                                <div class="projects__project-name">
                                                    features
                                                </div>
                                            </li>
                                            <li>
                                                <div class="projects__project-name">
                                                    hotfix
                                                </div>
                                            </li>
                                            <li>
                                                <div class="projects__project-name">
                                                    release
                                                </div>
                                            </li>
                                        </ul>
                                    </li>
                                </ul>
                            </div>
                        </td>
                    </tr>
                </table>
            </section>
            <footer class="modal__footer">
                <button class="button js-cancel">Close</button>
                <button class="button button-action js-save">Save</button>
            </footer>
        </div>
    </div>
</body>
</html>
